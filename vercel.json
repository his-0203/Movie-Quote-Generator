{
  "version": 2,
  "builds": [
    {
      "src": "ads.txt",
      "use": "@vercel/static"
    },
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 300
      }
    },
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/ads.txt",
      "dest": "/ads.txt"
    },
    {
      "src": "/api/proxy",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    }
  ]
}
